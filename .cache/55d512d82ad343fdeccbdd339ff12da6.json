f (typeof cell !== 'undefined') {\r\n          var cellText = this.parser.parseCell.call(this, cell);\r\n          csvLine.push(cellText);\r\n        }\r\n        if (rowSpan > 1) {\r\n          allSpans[spanIdx.toString()] = [rowSpan - 1, cellText];\r\n        }\r\n        spanIdx += 1;            \r\n        \r\n      }\r\n    }\r\n    result += csvLine.join() + '\\n';\r\n  }\r\n  return result\r\n}\r\n\r\nexport default parser;"},"lineCount":null}},"hash":"d3c3de3542709bcd59da50446853095a","cacheData":{"env":{}}}